{
  "name": "widt-labels",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "docker compose up -d && docker attach widt-reacties-frontend-1",
    "stop": "docker compose down",
    "open:frontend": "docker attach widt-reacties-frontend-1",
    "open:backend": "docker attach widt-reacties-backend-1",
    "edit:frontend": "docker exec -it widt-reacties-frontend-1 sh",
    "edit:backend": "docker exec -it widt-reacties-backend-1 sh",
    "dockerdev": "nuxt dev --port=4444",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "nuxt": "3.0.0"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/core": "^9.11.1",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "vite-plugin-markdown": "^2.1.0"
  }
}
